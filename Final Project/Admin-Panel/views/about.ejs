<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Layout Example</title>
    <style>
        body, html {
            margin: 0;
            
            height: 100%;
            width: 100%;
            display: flex;
        }
        .container2 {
            margin: 0;
            padding: 0;
            height: 100%;
            width: 100%;
       
        }

        
        .part-four {
          
            overflow: hidden;
        }
        .part-two {
            
            color: white;
            width: 100%;
            height: 12%;
            background-color: #202220;
            
            
      
        display: flex;
    flex-direction: row-reverse;
    justify-content: flex-start;
    align-items: center;
    justify-content: space-between;
        } 
        .part-twos{
            width: 50%;
            
            
            
        
        display: flex;
    flex-direction: row-reverse;
    justify-content: flex-start;
    align-items: center;
            justify-content:flex-end;
            margin-left: 25px;

        }
       
       

        

     

        .row2 {
            justify-content: flex-end; 
            display: flex;
            align-items: center;
            text-decoration: none;

        
 
        }
        
        .box2 {
           
           
            background-color: #202220;
   
            margin-right: 25px;
            display: flex;
            
            
            
        }
        .box2 a{
            text-decoration: none;
        }

       

        .box2 p:hover {
            color: rgb(238, 231, 231);
            
            transform: scale(1.02);

            
        }

       

        .nav1 {
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
            width: 100%;
            height: 100%;
        }

        .nav-left {
            float: left;
            display: flex;
            justify-content: center;
            align-items: center;
            text-align: center;
            padding: 2px;
        }

        .nav-left img:hover {
            box-shadow: 0 0 2px 1px rgba(0, 140, 186, 0.5);
        }

        .nav1 a {
            padding: 2px;
        }


        .small-icon {
            padding: 0% 1% 0% 1%;
            width: 50px;
            height: 50px;
            object-fit: cover;
            border-radius: 50%;
            margin: 5px;
        }

        img {
            width: 50px;
            height: 50px;
            object-fit: cover;
            border-radius: 50%;
            margin: 5px;
        }

    
.logo_write{
    color: White;
   
    font-size: 25px;
}


    </style>
</head>
<body>
    <div class="container2">

        <div class="part-two">
            <div class="row2">
                <div class="box2">
                    <a href="/dash_board">
                        <p class="logo_write">Home</p>
                    </a>
                </div>
            </div>
            <div class="part-twos">
                <div><%= user.name %></div>
            <% photos.forEach(photo => { %>
                <img class="small-icon" src="data:image/<%= photo.mime_type %>;base64,<%= Buffer.from(photo.photo).toString('base64') %>" alt="Photo">
            <% }) %>
        </div>

            </div>
            



        
        

           

            

                
                <div class="part-four">
                    <div class="nav1">
                        <div class="nav-left"><a href="/Data/Data-Panel.html">
                            <img src="/fb.png" width="200" height="200">
                        </a></div>
                        <div class="nav-left"><a href="/Data/Data-Panel.html">
                            <img src="/twitter.png" width="200" height="200">
                        </a></div>
                        <div class="nav-left"><a href="/Data/Data-Panel.html">
                            <img src="/linkedin.png" width="200" height="200">
                        </a></div>
                    </div>
                    <br>
                    <br>
                
                
            
        
           
           
    </div>
</body>
</html>
